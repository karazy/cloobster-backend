<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>eatsense-test</application>
	<version>2</version>

	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties" />
		<property name="net.karazy.channels.cockpit.timeout" value="480"/>
		<property name="net.karazy.channels.app.timeout" value="480"/>
	</system-properties>

	<inbound-services>
		<service>channel_presence</service>
	</inbound-services>
	
	<threadsafe>true</threadsafe>

	<!-- <static-files> <include path="**/app/*" /> </static-files> -->
	<static-files> 
        <include path="facebookchannel.html" expiration="3000d"/> 
    </static-files>
</appengine-web-app>
