<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
   <cron>
     <url>/cron/cleantokens</url>
     <description>Delete expired access tokens.</description>
     <schedule>every 24 hours</schedule>
   </cron>
   <cron>
     <url>/cron/generatedailycounters</url>
     <description>Generate daily counter reports.</description>
     <schedule>every day 00:01</schedule>
   </cron>
   <cron>
     <url>/cron/checkcockpits</url>
     <description>Check online status of cockpit channel connections.</description>
     <schedule>every 30 minutes</schedule>
   </cron>
   <cron>
     <url>/cron/checkinactivecheckins</url>
     <description>Check last activity of checkins clean or notify the cockpit.</description>
     <schedule>every 12 hours</schedule>
   </cron>
</cronentries>
